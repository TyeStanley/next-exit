<section class="dashboard-page">
  <section class='hero'>
    <div class='hero-cta'>
      {{#if loggedIn}}
        <h2>{{session.username}}'s Stats</h2>
        <div id="stat-container">
          <p class="p-stats">Comments: {{session.comments}}</p>
          <p class="p-stats">Posts: {{session.posts}}</p>
          <p class="p-stats">Likes: {{session.likes}}</p>
        </div>
      {{else}}
        <h2>You are not Logged In!</h2>
        <a href='/login'>login</a>
      {{/if}}
    </div>
  </section>

</section>